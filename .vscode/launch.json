            "program": "${workspaceRoot}/build/src/main.exe",
            "args": [],
            "stopAtEntry": false,
            "externalConsole": true,
            "cwd": "${fileDirname}",
            "environment": [],
            "MIMode": "gdb",
            "miDebuggerPath": "D:\\Xander Files\\program files\\msys64\\mingw64\\bin\\gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
